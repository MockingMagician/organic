{
    "name": "mocking-magician/organic",
    "description": "File manager library",
    "license": "CC-BY-SA-4.0",
    "homepage": "https://github.com/MockingMagician/organic",
    "keywords": [
        "file system", "file manager", "file", "directory", "file info", "secured and real-time file info"
    ],
    "authors": [
        {
            "name": "Marc MOREAU",
            "email": "moreau.marc.web@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MockingMagician\\Organic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MockingMagician\\Organic\\Tests\\": "tests/src/",
            "MockingMagician\\Organic\\PHPUnitExt\\": "tests/ext/"
        }
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8 || ^7",
        "phpstan/phpstan": "^0.11",
        "friendsofphp/php-cs-fixer": "v2.12.12",
        "phpbench/phpbench": "@dev",
        "fzaninotto/faker": "^1.8"
    }
}
